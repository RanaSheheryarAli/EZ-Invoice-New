<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Outer white circle (border) -->
    <item>
        <shape android:shape="oval">
            <solid android:color="#F7F8FA"/>
            <size android:width="62dp" android:height="62dp"/>
        </shape>
    </item>

    <!-- Inner teal circle -->
    <item android:top="2dp" android:bottom="3dp" android:left="2dp" android:right="2dp">
        <shape android:shape="oval">
            <solid android:color="@color/buttoncolor"/>
        </shape>
    </item>
</layer-list>